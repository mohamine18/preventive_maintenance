<% if (paginate.lastPage !== 1) { %>
    <nav aria-label="Page navigation">
        <ul class="pagination justify-content-center">
            <% if (paginate.hasPreviousPage) { %>
                <li class="page-item"><a class="page-link" href="<%= paginate.urlPrefix %><%= paginate.currentPage - 1 %>">Previous</a></li>
                <% } else { %>
                <li class="page-item disabled"><a class="page-link" href="#">Previous</a></li>
            <% } %>
    
            <% for( let index = 1; index <= paginate.lastPage; index++ ) { %>
                <li class="page-item"><a class="page-link <% if (index === paginate.currentPage) { %> active <% } %>" href="<%= paginate.urlPrefix %><%= index %>"><%= index %></a></li>
            <% } %>
    
            <% if (paginate.hasNextPage) { %>
                <li class="page-item"><a class="page-link" href="<%= paginate.urlPrefix %><%= paginate.currentPage + 1 %>">Next</a></li>
                <% } else { %>
                <li class="page-item disabled"><a class="page-link" href="#">Next</a></li>
            <% } %>
        </ul>
    </nav>
<% } %>